            <property name="streetName" type="String" label="Street API Name" description="The API name of the field representing Street data for the child record."/>
            <property name="cityName" type="String" label="City API Name" description="The API name of the field representing City/Suburb data for the child record."/>
            <property name="stateName" type="String" label="State API Name" description="The API name of the field representing State data for the child record."/>
            <property name="postcode" type="String" label="Postcode API Name" description="The API name of the field representing postal/zip code data for the child record."/>
            <property name="countryName" type="String" label="Country API Name" description="The API name of the field representing Country data for the child record."/>
            <property name="enableCircle" type="Boolean" label="Enable circle indicator on map?" description="If this is ticked, the map marker is replaced with a transclucent yellow circle. If map is not enabled, this does nothing."/>
